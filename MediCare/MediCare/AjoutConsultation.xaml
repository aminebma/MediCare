<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MediCare"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="MediCare.AjoutConsultation"
        mc:Ignorable="d"
        Title="Consultation" Height="772.5" Width="1107.5">
    <Grid Margin="0,-12,0,12" Background="#FFE5E8ED">
        <TextBox x:Name="nomPatientT" HorizontalAlignment="Left" Height="40" Margin="246,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" Foreground="#FFADA49F" BorderBrush="#FF6A94A8" CaretBrush="#FF6990A1" SelectionBrush="#FF6A97AC" mah:TextBoxHelper.AutoWatermark="True" mah:TextBoxHelper.Watermark="Nom du Patient"/>
        <TextBox x:Name="prenomPatientT" HorizontalAlignment="Left" Height="40" Margin="247,155,0,0" TextWrapping="Wrap" Text="Prénom du patient" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" Foreground="#FF070004" CaretBrush="#FF6A94A8" SelectionBrush="#FF6992A4" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="nomMedecinT" HorizontalAlignment="Left" Height="40" Margin="245,239,0,0" TextWrapping="Wrap" Text="Nom du Médecin" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" Foreground="#FF070004" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="prenomMedecinT" HorizontalAlignment="Left" Height="40" Margin="245,319,0,0" TextWrapping="Wrap" Text="Prénom du Médecin" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" Foreground="#FF070004" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="diagnosticT" HorizontalAlignment="Left" Height="40" Margin="247,400,0,0" TextWrapping="Wrap" Text="Diagnostic" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" Foreground="#FF070004" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="descriptionT" HorizontalAlignment="Left" Height="40" Margin="244,480,0,0" TextWrapping="Wrap" Text="Description" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" Foreground="#FF070004" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="medicamentT" HorizontalAlignment="Left" Height="40" Margin="799,410,0,0" TextWrapping="Wrap" Text="Medicament" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="doseT" HorizontalAlignment="Left" Height="40" Margin="799,492,0,0" TextWrapping="Wrap" Text="Dose" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="indicationT" HorizontalAlignment="Left" Height="40" Margin="799,560,0,0" TextWrapping="Wrap" Text="Idication" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="radioT" HorizontalAlignment="Left" Height="40" Margin="247,559,0,0" TextWrapping="Wrap" Text="Radio" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" BorderBrush="#FF6A97AC" CaretBrush="#FF6A97AC" SelectionBrush="#FF6A94A8"/>
        <TextBox x:Name="certificatT" HorizontalAlignment="Left" Height="40" Margin="795,80,0,0" TextWrapping="Wrap" Text="Certificat" VerticalAlignment="Top" Width="242" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" SelectionBrush="#FF688A9A" BorderBrush="#FF6A97AC" CaretBrush="#FF6A97AC"/>
        <TextBox x:Name="lettreT" HorizontalAlignment="Left" Height="40" Margin="797,160,0,0" TextWrapping="Wrap" Text="Lettre" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SpellCheck.IsEnabled="True" SelectionBrush="#FF688A9A" BorderBrush="#FF6A97AC" CaretBrush="#FF6A97AC"/>
        <TextBox x:Name="scannerT" HorizontalAlignment="Left" Height="40" Margin="797,238,0,0" TextWrapping="Wrap" Text="Scanner" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <TextBox x:Name="bilanT" HorizontalAlignment="Left" Height="40" Margin="797,320,0,0" TextWrapping="Wrap" Text="Bilan" VerticalAlignment="Top" Width="240" Background="{x:Null}" FontSize="18" FontFamily="Century Gothic" SelectionBrush="#FF6A97AC" CaretBrush="#FF6A97AC" BorderBrush="#FF6A97AC"/>
        <Label x:Name="NomPatient" Content="Nom du patient :" HorizontalAlignment="Left" Height="42" Margin="35,79,0,0" VerticalAlignment="Top" Width="160" FontSize="18" RenderTransformOrigin="0.906,-0.274" FontFamily="Century Gothic"/>
        <Label x:Name="PrenomPatient" Content="Prénom du patient :" HorizontalAlignment="Left" Height="40" Margin="35,156,0,0" VerticalAlignment="Top" Width="184" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="NomMedecin" Content="Nom du Médecin :" HorizontalAlignment="Left" Height="42" Margin="33,237,0,0" VerticalAlignment="Top" Width="174" FontSize="18" FontFamily="Century Gothic" RenderTransformOrigin="0.497,-0.845"/>
        <Label x:Name="PrenomMedecin" Content="Prénom du Médecin:" HorizontalAlignment="Left" Height="42" Margin="32,319,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Diagnostic" Content="Diagnostic :" HorizontalAlignment="Left" Height="42" Margin="27,402,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Description" Content="Description :" HorizontalAlignment="Left" Height="42" Margin="31,479,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Medicament" Content="Médicament :" HorizontalAlignment="Left" Height="42" Margin="573,409,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Dose" Content="Dose :" HorizontalAlignment="Left" Height="42" Margin="573,489,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Indication" Content="Indication :" HorizontalAlignment="Left" Height="42" Margin="569,560,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Certificat" Content="Certificat :" HorizontalAlignment="Left" Height="42" Margin="575,78,0,0" VerticalAlignment="Top" Width="200" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Lettre" Content="Lettre :" HorizontalAlignment="Left" Height="42" Margin="575,160,0,0" VerticalAlignment="Top" Width="200" FontSize="18" FontFamily="Century Gothic"/>
        <Label x:Name="Scanner" Content="Scanner :" HorizontalAlignment="Left" Height="42" Margin="575,238,0,0" VerticalAlignment="Top" Width="200" FontSize="18" FontFamily="Century Gothic" Background="{x:Null}"/>
        <Label x:Name="Bilan" Content="Bilan :" HorizontalAlignment="Left" Height="42" Margin="575,320,0,0" VerticalAlignment="Top" Width="200" FontSize="18" FontFamily="Century Gothic" Background="{x:Null}"/>
        <Label x:Name="Radio" Content="Radio :" HorizontalAlignment="Left" Height="42" Margin="32,561,0,0" VerticalAlignment="Top" Width="192" FontSize="18" FontFamily="Century Gothic"/>
        <Button x:Name="Add_Consultation" Content="ADD" HorizontalAlignment="Left" Height="52" Margin="593,641,0,0" VerticalAlignment="Top" Width="320" FontWeight="Normal" FontFamily="Century Gothic" FontSize="20" Background="#FFADA49F" Click="Add_Consultation_Click" Foreground="#FF000003"/>
    </Grid>
</Window>

