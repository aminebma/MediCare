<UserControl x:Class="MediCare.MenuPatient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MediCare"
        mc:Ignorable="d"
        Height="548" Width="1072">
    <Grid>
        <Button x:Name="Rech_pat" Content="Rechercher un patient" HorizontalAlignment="Left" Height="74" Margin="448,82,0,0" VerticalAlignment="Top" Width="248" Background="#FF77B3B4" Click="Rech_pat_Click"/>
        <Button x:Name="Ajout_pat" Content="Ajouter un patient" HorizontalAlignment="Left" Height="74" Margin="448,204,0,0" VerticalAlignment="Top" Width="248" Background="#FF77B3B4" Click="Ajout_pat_Click"/>
        <Button x:Name="Supr_pat" Content="Supprimer un patient" HorizontalAlignment="Left" Height="74" Margin="448,328,0,0" VerticalAlignment="Top" Width="248" Background="#FF77B3B4" Click="Supr_pat_Click"/>
    </Grid>
</UserControl>
