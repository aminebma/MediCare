                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image Margin="41,230,645,254" Source="ressources/images/bulles_3.PNG" Stretch="Fill" RenderTransformOrigin="0.58,0.533" Opacity="0.3">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="0.999" ScaleX="1.001"/>
                        <SkewTransform AngleY="-0.489"/>
                        <RotateTransform Angle="-12.072"/>
                        <TranslateTransform X="-0.045" Y="-0.135"/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </Grid>
    </Grid>
</UserControl>
