                    <TransformGroup>
                        <ScaleTransform ScaleY="0.996" ScaleX="1.004"/>
                        <SkewTransform AngleY="0.919"/>
                        <RotateTransform Angle="25.338"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image Margin="0,245,35,225" Source="ressources/images/bulles_2.PNG" Stretch="Fill" HorizontalAlignment="Right" Width="46" Opacity="0.4" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="0.999" ScaleX="1.001"/>
                        <SkewTransform AngleY="-0.392"/>
                        <RotateTransform Angle="-9.571"/>
