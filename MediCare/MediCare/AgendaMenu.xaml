                </Image.RenderTransform>
            </Image>
            <Image Margin="0,433,642,9" Source="ressources/images/bulles_2.PNG" Stretch="Fill" HorizontalAlignment="Right" Width="59" Opacity="0.4" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="0.999" ScaleX="1.001"/>
                        <SkewTransform AngleY="-0.446"/>
                        <RotateTransform Angle="-10.959"/>
