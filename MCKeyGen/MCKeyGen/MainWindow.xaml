<Window x:Name="main" x:Class="MCKeyGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MCKeyGen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="563.158" PreviewKeyDown="main_PreviewKeyDown">
    <Grid>
        <PasswordBox x:Name="pass" HorizontalAlignment="Left" Height="33" Margin="168,284,0,0" VerticalAlignment="Top" Width="250" GotFocus="pass_GotFocus"/>
        <TextBox x:Name="user" HorizontalAlignment="Left" Height="33" Margin="168,225,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="250" TextChanged="user_TextChanged" GotFocus="user_GotFocus"/>
        <Image x:Name="im" HorizontalAlignment="Left" Height="154" Margin="211,34,0,0" VerticalAlignment="Top" Width="164" Source="ressources/debut.JPG"/>
        <Label Content="Username" HorizontalAlignment="Left" Height="33" Margin="63,225,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="33" Margin="63,284,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
